import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {//this is a switch program
	    
	    Scanner s = new Scanner(System.in);
	    String fruit =s.next();
	    switch(fruit)
	    {
	        case "mango":
	        System.out.println("king of fruit");
	        break;
	        case "apple":
	            System.out.println("red fruit");
	            break;
	            case "banana":
	            System.out.println("healthy fruit");
	            break;
	            case "red":
	                System.out.println("vest colour of world");
	                break;
	                default:
	                System.out.println("please select the food");
	                break;
	    }
		System.out.println("");
	}
}

